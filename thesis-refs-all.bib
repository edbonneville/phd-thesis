}

@article{vonhippelHowManyImputations2020,
  title = {How {{Many Imputations Do You Need}}? {{A Two-stage Calculation Using}} a {{Quadratic Rule}}},
  shorttitle = {How {{Many Imputations Do You Need}}?},
  author = {{von Hippel}, Paul T.},
  year = {2020},
