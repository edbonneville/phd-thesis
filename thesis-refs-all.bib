  author = {Minculescu, Lia and Marquart, Hanne Vibeke and Friis, Lone Smidstrup and Petersen, Soeren Lykke and Schi{\o}dt, Ida and Ryder, Lars Peter and Andersen, Niels Smedegaard and Sengeloev, Henrik},
