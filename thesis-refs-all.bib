  author = {Horowitz, {\relax MM} and Gale, {\relax RP} and Sondel, {\relax PM} and Goldman, {\relax JM} and Kersey, J and Kolb, {\relax HJ} and Rimm, {\relax AA} and Ringden, O and Rozman, C and Speck, B},
